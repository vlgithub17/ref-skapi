{
  "name": "ref-skapi",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "prod": "vite --host --mode production",
    "build": "npm run build:ts; node script.cjs; vite build --mode production",
    "build:ts": "tsc",
    "build-check": "node script.cjs; run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --host",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "start": "serve dist"
  },
  "dependencies": {
    "aos": "^3.0.0-beta.6",
    "jsoncrawler": "^0.2.1",
    "mime": "^4.0.4",
    "skapi-js": "^1.0.201"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/tsconfig": "^0.5.1",
    "less": "^4.2.0",
    "npm-run-all2": "^6.1.1",
    "pug": "^3.0.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "serve": "^14.2.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vite-plugin-compression": "^0.5.1",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "vue-tsc": "^2.0.29"
  }
}
